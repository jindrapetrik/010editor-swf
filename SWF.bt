typedef struct {
    ubyte FillStyleType;
    switch (FillStyleType) {
        case 0x00:
            if ((CurrentTag == 2)       /* DefineShape  */
            || (CurrentTag == 22)) {    /* DefineShape2 */
                RGB Color;
            }
            else {              /* DefineShape3 or DefineShape4 */
                RGBA Color;
            }
            break;
        case 0x10:
        case 0x12:
            MATRIX GradientMatrix;
            GRADIENT Gradient;
            break;
        case 0x13:
            MATRIX GradientMatrix;
            FOCALGRADIENT Gradient;
            break;
        case 0x40:
        case 0x41:
        case 0x42:
        case 0x43:
            ushort BitmapId;
